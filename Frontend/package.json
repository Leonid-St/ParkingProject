        "@types/dotenv": "^8.2.0",
        "@types/node": "^12.20.24",
        "@types/react": "^17.0.20",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.1.8",
        "@types/redux-logger": "^3.0.9",
        "bootstrap": "^5.1.1",
        "broadcast-channel": "^4.2.0",
        "devextreme": "21.2",
        "devextreme-react": "21.2",
        "dotenv": "^8.2.0",
        "guid-typescript": "^1.0.9",
        "react": "^17.0.2",
        "react-bootstrap": "^2.0.0-rc.0",
        "react-dom": "^17.0.2",
        "react-player": "^2.9.0",
        "react-redux": "^7.2.5",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "react-video": "^1.5.3",
        "react-video-play": "^2.0.3",
        "redux": "^4.1.2",
        "redux-devtools-extension": "^2.13.9",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
