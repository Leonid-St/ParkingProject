    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "rules": {
                    "camelcase": "warn",
                    "no-debugger": "error",
                    "indent": [
                        "warn",
                        4
                    ],
                    "eol-last": [
                        "warn",
                        "always"
                    ],
                    "quotes": [
                        "warn",
                        "double"
                    ],
                    "semi": [
                        "warn",
                        "always"
                    ],
                    "no-multi-spaces": "warn"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
